<?xml version="1.0" encoding="utf-8"?>

<VoiceCommands xmlns="http://schemas.microsoft.com/voicecommands/1.2">

  <CommandSet xml:lang="en-US" Name="NetflixSearchCommandSet_en-us">
    <AppName> Netflix </AppName>
    <Example>find House of Cards</Example>
    <Command Name="FindTitle">
      <Example>find House of Cards</Example>
      <ListenFor>find {title}</ListenFor>
      <ListenFor RequireAppName="ExplicitlySpecified">find {title} on {builtin:AppName}</ListenFor>
      <ListenFor RequireAppName="ExplicitlySpecified">find {title} using {builtin:AppName}</ListenFor>
      <ListenFor RequireAppName="ExplicitlySpecified">find {title} with {builtin:AppName}</ListenFor>
      <Feedback>Netflix is finding {title}</Feedback>
      <Navigate Target="SearchResults"/>
    </Command>
    <Command Name="SearchTitle">
      <Example>search House of Cards</Example>
      <ListenFor>search {title}</ListenFor>
      <ListenFor RequireAppName="ExplicitlySpecified">search {title} on {builtin:AppName}</ListenFor>
      <ListenFor RequireAppName="ExplicitlySpecified">search {title} using {builtin:AppName}</ListenFor>
      <ListenFor RequireAppName="ExplicitlySpecified">search {title} with {builtin:AppName}</ListenFor>
      <Feedback>Netflix is searching for {title}</Feedback>
      <Navigate Target="SearchResults"/>
    </Command>
    <Command Name="SearchForTitle">
      <Example>search for House of Cards</Example>
      <ListenFor RequireAppName="BeforeOrAfterPhrase">search for {title}</ListenFor>
      <Feedback>Netflix is searching for {title}</Feedback>
      <Navigate Target="SearchResults"/>
    </Command>
    <Command Name="LookForTitle">
      <Example>look for House of Cards</Example>
      <ListenFor RequireAppName="BeforeOrAfterPhrase">look for {title}</ListenFor>
      <Feedback>Netflix is looking for {title}</Feedback>
      <Navigate Target="SearchResults"/>
    </Command>
    <Command Name="LookUpTitle">
      <Example>look up House of Cards</Example>
      <ListenFor RequireAppName="BeforeOrAfterPhrase">look up {title}</ListenFor>
      <Feedback>Netflix is looking up {title}</Feedback>
      <Navigate Target="SearchResults"/>
    </Command>
    <PhraseTopic Label="title" Scenario="Dictation"/>
  </CommandSet>


